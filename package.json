{
	"name": "@tobper/eon",
	"version": "0.0.17",
	"description": "Date library",
	"repository": "github:tobper/eon",
	"license": "ISC",
	"keywords": [
		"typescript",
		"calendar",
		"date"
	],
	"author": "Tobias Persson",
	"type": "module",
	"main": "lib/index.js",
	"types": "lib/types/index.d.ts",
	"files": [
		"lib",
		"src",
		"!**/*.tsbuildinfo"
	],
	"packageManager": "pnpm@9.7.0",
	"publishConfig": {
		"@tobper:registry": "https://npm.pkg.github.com"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc --watch",
		"spelling": "cspell -c .vscode/cSpell.json --no-progress **",
		"test": "vitest",
		"lint": "eslint .",
		"lint:fix": "pnpm -r run lint:fix",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.0",
		"@types/eslint": "^9.6.0",
		"@types/eslint__js": "^8.42.3",
		"cspell": "^8.13.3",
		"eslint": "^9.8.0",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.1.0",
		"vitest": "^2.0.5"
	}
}
